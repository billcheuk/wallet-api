    "coverage": "yarn test --coverage",
    "clean": "rm -rf node_modules/ bin/",
    "format": "yarn prettier && yarn eslint",
    "prettier": "npx prettier 'src/**/*.ts' --write",
    "eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
